        // ==============================================================
        // Container fluid
        // ==============================================================
        .container-fluid
          // Start Page Content
          // ==============================================================
          .row(style="padding-top:30px;", ng-app='customerApp', ng-controller='customerController')
            .col-12
              .card
                .card-body.p-b-0
                  ul.nav.nav-tabs.customtab(role='tablist')
                    li.nav-item
                      a.nav-link.show.active(data-toggle='tab', href='#home2', role='tab', aria-selected='true')
                        span.hidden-sm-up
                          i.ti-home
                        span.hidden-xs-down Devices List
                    li.nav-item
                      a.nav-link.show(data-toggle='tab', href='#profile2', role='tab', aria-selected='false')
                        span.hidden-sm-up
                          i.ti-user
                        span.hidden-xs-down Upload Devices

                  // Tab panes
                  .tab-content
                    #home2.tab-pane.show.active(role='tabpanel')
                      .p-20
                        h4.card-title {{provider.name}} Devices
                        h6.card-subtitle Devices details

                        .table-responsive.m-t-40
                          table.table.table-striped.table-bordered.display.nowrap.table-hover(datatable='ng', dt-options='dataTableOpt', width='100%', role='grid')
                            thead
                              tr(role='row')
                                th
                                th Name
                                th IMEI
                                th Model
                                th Admission Date
                                th Ubication
                            tbody
                              tr(role='row', ng-repeat='device in devices', ng-model="selectedDevices")
                                td
                                  .md-checkbox.chk-col-indigo
                                    input(id="{{device.id}}", type='checkbox', ng-click="clickedItem(device.imei)")
                                    label(for='{{device.id}}')
                                td {{ device.name }}
                                td {{ device.imei }}
                                td {{ device.mpn }}
                                td {{ device.admission_date | date:'dd/MM/yyyy' }}
                                td {{ device.ubication }}

                        .p-20
                          button.btn.btn-info(type='submit', ng-if="selectedDevices.length !== 0")
                            i.fa.fa-eye
                            |  View  Assigment
                          button.btn.btn-success(type='submit', ng-if="selectedDevices.length !== 0", style="padding-left:10px;", ng-click="sm()")
                            i.fa.fa-plus-circle
                            |  Create Assigment

                        .p-20
                          #exampleModal.modal.fade.show(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel1', aria-modal='true', style='display: block; padding-right: 15px;', ng-show="showModal")
                            .modal-dialog(role='document')
                              .modal-content
                                .modal-header
                                  h4#exampleModalLabel1.modal-title Add Provider
                                  button.close(type='button', data-dismiss='modal', aria-label='Close' ng-click="sm()")
                                    span(aria-hidden='true') ×
                                .modal-body
                                  form
                                    .form-group
                                      h4 Selected Devices (IMEI's):
                                      .md-checkbox.chk-col-indigo(ng-repeat="imd in selectedDevices")
                                        input(id="{{imd}}", checked ,type='checkbox' )
                                        label(for='{{imd}}') {{imd}}
                                    .form-group
                                      label.control-label(for='recipient-name') Description:
                                      input#recipient-name1.form-control(type='text', ng-model="description")
                                    .form-group
                                      label.control-label(for='message-name') User Assigned:
                                      input#message-text1.form-control(ng-model="email")
                                    .form-group
                                      label.m-t-40 Default Material End Date
                                      input.form-control(type='date', placeholder='dd/mm/yyyy', ng-model="admission_date")

                                .modal-footer
                                  button.btn.btn-info(type='button', data-dismiss='modal', ng-click="sm()") Close
                                  button.btn.btn-success(type='button', ng-click="saveProvider()") Save
                          .modal-backdrop.fade.show(ng-show="showModal")


                    #profile2.tab-pane.p-20.show(role='tabpanel')
                      .vtabs.customvtab
                        ul.nav.nav-tabs.tabs-vertical(role='tablist')
                          li.nav-item
                            a.nav-link.active(data-toggle='tab', href='#home3', role='tab')
                              span.hidden-sm-up
                                i.ti-home
                              span.hidden-xs-down One
                          li.nav-item
                            a.nav-link(data-toggle='tab', href='#profile3', role='tab')
                              span.hidden-sm-up
                                i.ti-user
                              span.hidden-xs-down From .csv
                                             // Tab panes
                        .tab-content
                          #home3.tab-pane.active(role='tabpanel')
                            .p-20
                              form(action='#')
                                .form-body
                                  h3.card-title Device Info
                                  hr
                                  .row.p-t-20
                                    .col-md-4
                                      .form-group
                                        label.control-label Comercial Name
                                        input#firstName.form-control( ng-model="name")
                                        small.form-control-feedback  This is inline help
                                      // /span
                                    .col-md-4
                                      .form-group
                                        label.control-label IMEI
                                        input#lastName.form-control(type='number', ng-model="imei")
                                        //small.form-control-feedback(style="display:none")  This field has error.
                                    .col-md-4
                                      .form-group
                                        label.control-label Model
                                        input#lastName.form-control(type='text', ng-model="mpn")
                                    // /span
                                    // /row
                                  .row
                                    .col-md-6
                                      .form-group
                                        label.control-label Ubication
                                        input#firstName.form-control(type='text', ng-model="ubication", placeholder='In Stock', style='background-image: url("data:image/png;base64,"); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;')
                                        small.form-control-feedback  This is inline help
                                    // /span
                                      .col-md-6
                                        .form-group
                                          label.control-label Admission Date
                                          input.form-control(type='date', placeholder='dd/mm/yyyy', ng-model="admission_date")
                                    .row
                                      .col-md-4
                                        button.btn.btn-success(type='submit', ng-click="saveDevice()")
                                          i.fa.fa-check
                                          |  Save
                                       .col-md-4
                                          button.btn.btn-inverse(type='button') Cancel

                          #profile3.tab-pane.p-20(role='tabpanel')
                            form.row(action='')
                              .col-md-12
                                input.dropify(type='file', data-default-file='')


